{
  "name": "@shared-infra/storage-service",
  "version": "1.0.0",
  "description": "Deployable storage service for photo management system",
  "main": "dist/server.js",
  "files": [
    "dist/**/*",
    "Dockerfile",
    "docker-compose.yml"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only --clear src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "docker:build": "docker build -t storage-service .",
    "docker:run": "docker run -p 3001:3001 storage-service",
    "docker:compose": "docker-compose up --build"
  },
  "keywords": [
    "storage",
    "photos",
    "service",
    "api",
    "express",
    "minio",
    "sqlite"
  ],
  "author": "Your Organization",
  "license": "MIT",
  "dependencies": {
    "@shared-infra/storage-core": "^1.0.0",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "typescript": "^5.3.0",
    "jest": "^29.7.0",
    "@types/jest": "^30.0.0",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.2",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^6.14.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/receipt-processor.git",
    "directory": "photo-management/shared-infra/packages/storage-service"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
